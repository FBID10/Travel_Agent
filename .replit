modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_gemini_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Travel Agent API"

[[workflows.workflow]]
name = "Travel Agent API"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd multi_agent_weather && ./start.sh"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "vm"
run = ["bash", "-c", "cd multi_agent_weather && uv run --python 3.12 uvicorn weather_agent.agent:a2a_app --host 0.0.0.0 --port 8000 & sleep 2 && uv run --python 3.12 adk web . --host 0.0.0.0 --port 5000"]
